{
    "name": "tda/laravel-resellerinterface",
    "description": "Laravel package for Reserler interface API package",
    "type": "library",
    "require": {
        "resellerinterface/api-client-php": "^1.0",
        "php": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tda\\LaravelResellerinterface\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Davi Leichsenring",
            "email": "davileichs@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tda\\LaravelResellerinterface\\ResellerinterfaceServiceProvider"
            ],
            "aliases": {
                "Resellerinterface": "Tda\\LaravelResellerinterface\\Facades\\Resellerinterface"
            }
        }
    },
    "minimum-stability": "dev"
}
